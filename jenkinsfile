pipeline {
    agent any
    
     realtimeJUnit('**/target/surefire-reports/TEST-*.xml') {
    sh 'mvn -Dmaven.test.failure.ignore=true clean verify'
}

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
   
}
