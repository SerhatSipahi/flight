pipeline {
   
   agent any
    

    stages {
        stage('Cleaning Stages') {
            steps {
                sh 'mvn clean'
            }
        }
        stage('Testing Stage') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Packaging Stage') {
            steps {
                sh 'mvn package'
            }
        }
    }
   
}
